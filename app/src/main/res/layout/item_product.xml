<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tvCountPlaceBackProduct"
        style="@style/DataTextSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/tvNameProduct"
        tools:text="352,568" />

    <TextView
        android:id="@+id/tvCountProduct"
        style="@style/DataTextSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintEnd_toStartOf="@+id/textView17"
        app:layout_constraintTop_toBottomOf="@+id/tvNameProduct"
        tools:text="352,568" />

    <TextView
        android:id="@+id/tvCountPlaceProduct"
        style="@style/DataTextSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvNameProduct"
        tools:text="352,568" />

    <TextView
        android:id="@+id/tvCountPalletProduct"
        style="@style/DataTextSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvCountPlaceProduct"
        tools:text="352,568" />

    <TextView
        android:id="@+id/tvCountBackProduct"
        style="@style/DataTextSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toEndOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/tvNameProduct"
        tools:text="352,568" />

    <TextView
        android:id="@+id/tvNumberView"
        style="@style/DataTextMedium"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="1" />

    <TextView
        android:id="@+id/tvNameProduct"
        style="@style/SectionDataText"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tvNumberView"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Говядина на кости  мягкая очень, дата прихода 10.11.2019" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:text="К"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvCountBackProduct"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:text="М"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvCountBackProduct"
        app:layout_constraintStart_toEndOf="@+id/tvCountBackProduct" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="М"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvCountPlaceProduct"
        app:layout_constraintEnd_toStartOf="@+id/tvCountPlaceProduct" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="П"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvCountPalletProduct"
        app:layout_constraintEnd_toStartOf="@+id/tvCountPalletProduct" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="К"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvCountProduct"
        app:layout_constraintEnd_toStartOf="@+id/tvCountProduct" />


</androidx.constraintlayout.widget.ConstraintLayout>